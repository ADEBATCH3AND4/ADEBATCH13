						"value": "[parameters('trigger1_properties_pipeline7_parameters_output_Table')]"
					},
					"trigger2_properties_pipeline7_parameters_input_Table": {
						"value": "[parameters('trigger2_properties_pipeline7_parameters_input_Table')]"
					},
					"trigger2_properties_pipeline7_parameters_output_Table": {
						"value": "[parameters('trigger2_properties_pipeline7_parameters_output_Table')]"
					}
